#include <stdio.h>

int main() {
// Getting nums
    int a, b, c;
    printf("Enter nums for (a, b, c): ");
    scanf("%d %d %d", &a, &b, &c);
// Multiply and getting sign of it
    int mulofall = a * b * c;
    int s_mulofall = (mulofall > 0) - (mulofall < 0);

    printf("Sign is %d", s_mulofall);
    return 0;
}
